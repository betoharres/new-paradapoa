{
  "name": "paradapoa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache --watch",
    "lint": "eslint .",
    "format": "prettier-standard --staged --format"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.7",
    "@react-navigation/material-top-tabs": "^5.1.7",
    "@react-navigation/native": "^5.1.4",
    "@react-navigation/stack": "^5.2.9",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-appearance": "^0.3.3",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.7.1",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-snap-carousel": "^3.9.0",
    "react-native-sqlite-storage": "^5.0.0",
    "react-native-vector-icons": "^6.6.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.4.0",
    "babel-plugin-root-import": "^6.5.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.2",
    "metro-react-native-babel-preset": "^0.59.0",
    "prop-types": "^15.7.2",
    "react-native-testing-library": "^1.13.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setupFilesAfterEnv.js"
    ],
    "testMatch": [
      "<rootDir>/app/**/*.spec.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|react-navigation|@react-navigation/.*|react-native-gesture-handler)"
    ],
    "haste": {
      "defaultPlatform": "android",
      "platforms": [
        "ios",
        "android",
        "native"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.--format": "prettier --write"
  }
}
